{
  "name": "pomodory",
  "private": true,
  "version": "1.0.0",
  "description": "Helps you stay focus using the Pomodoro technique",
  "main": "dist/index.js",
  "author": "Nicolas Carlo <nicolascarlo.espeon@gmail.com>",
  "license": "MIT",
  "keywords": [
    "pomodoro",
    "dory",
    "productivity",
    "timer"
  ],
  "devDependencies": {
    "@types/jest": "23.1.4",
    "@types/node": "10.5.2",
    "@types/uuid": "3.4.3",
    "husky": "1.0.0-rc.12",
    "jest": "23.3.0",
    "prettier": "1.13.7",
    "pretty-quick": "1.6.0",
    "ts-jest": "23.0.0",
    "tsc": "1.20150623.0",
    "typescript": "2.9.2"
  },
  "scripts": {
    "test": "jest",
    "prestart": "tsc",
    "start": "node dist/index.js"
  },
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "roots": [
      "<rootDir>/src"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "dependencies": {
    "chalk": "2.4.1",
    "inquirer": "6.0.0",
    "uuid": "3.3.2"
  }
}
